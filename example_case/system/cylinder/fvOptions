FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      cylinder;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitTemperature
{
    type	limitTemperature;
    active	true;
    limitTemperatureCoeffs
    {
        selectionMode all;
        min	200;
        max	700;
    }
}

