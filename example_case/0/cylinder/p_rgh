FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;			//scientific notation ist auch erlaubt

boundaryField
{
    cylinder_to_fluid				//was steht in Kontakt mit was?
    {
        type            fixedFluxPressure;
    }
}

