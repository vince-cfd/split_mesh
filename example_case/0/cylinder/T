FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.006/cylinder";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 200;

boundaryField
{
	fluid
        {
            type            zeroGradient;
            value           uniform 200;
        }

        cylinder_to_fluid
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            value           uniform 200;
        }
}


